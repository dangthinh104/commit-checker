{
  "name": "dangthinh104/commit-checker",
  "description": "Check coding standard & code syntax with Git pre-commit hook.",
  "type": "package",
  "license": "MIT",
  "keywords": [
    "Dangthinh104"
  ],
  "authors": [
    {
      "name": "Quoc Thinh",
      "email": "dangluuquocthinh@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "dangthinh104\\CommitChecker\\": "src"
    }
  },
  "require": {
    "php": "^7.1.3",
    "laravel/framework": "^5.6|^6|^7|^8",
    "squizlabs/php_codesniffer": "^3.3",
    "phpmd/phpmd": "^2.8.2"
  },
  "extra": {
    "laravel": {
      "providers": [
        "dangthinh104\\CommitChecker\\Providers\\CommitCheckerServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable"
}
